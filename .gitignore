# .gitignore for Grails 1.2 and 1.3

# web application files
/petitionsweb/web-app/WEB-INF/classes

# default HSQL database files for production mode
/petitionsweb/prodDb.*

# general HSQL database files
*Db.properties
*Db.script
/petitionsweb/*Db.properties
/petitionsweb/*Db.script

# logs
/stacktrace.log
/petitionsweb/test/reports
/petitionsweb/logs

# project release file
/petitionsweb/*.war

# plugin release files
/petitionsweb/*.zip
/petitionsweb/plugin.xml

# older plugin install locations
/petitionsweb/plugins
/petitionsweb/web-app/plugins

# "temporary" build files
/petitionsweb/target


petitionsweb/.classpath
petitionsweb/.project
petitionsweb/.settings/
